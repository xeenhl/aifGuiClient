<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<TabPane fx:id="tabs" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.aif.gui.controllers.ResultPaneController">
  <tabs>
    <Tab fx:id="ssplit" text="Split">
         <content>
            <Pane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <TextArea fx:id="ssplitResult" layoutX="-3.0" layoutY="-3.0" prefHeight="360.0" prefWidth="600.0" />
               </children>
            </Pane>
         </content></Tab>
      <Tab fx:id="ess" text="Separators">
         <content>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <ListView fx:id="essGroup" prefHeight="200.0" prefWidth="200.0" />
                  <Separator orientation="VERTICAL" prefHeight="200.0" />
                  <TextArea fx:id="essCharacters" prefHeight="367.0" prefWidth="399.0" />
               </children>
            </HBox>
         </content>
      </Tab>
      <Tab fx:id="dbuild" text="Dictionary">
         <content>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <VBox minWidth="-Infinity" prefHeight="367.0" prefWidth="150.0">
                     <children>
                        <Label text="Basic token" textAlignment="CENTER" />
                        <ListView fx:id="dbuildBasicTokens" prefHeight="356.0" prefWidth="100.0" />
                     </children>
                  </VBox>
                  <Separator orientation="VERTICAL" prefHeight="200.0" />
                  <VBox prefHeight="367.0" prefWidth="517.0">
                     <children>
                        <Label text="Tokens:" />
                        <TextArea fx:id="dbuildTokens" prefHeight="367.0" prefWidth="494.0" />
                     </children>
                  </VBox>
               </children>
            </HBox>
         </content>
      </Tab>
      <Tab fx:id="tsplit" text="Tokens">
         <content>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Label text="Tokens:" />
                  <TextArea fx:id="tsplitTokens" prefHeight="413.0" prefWidth="600.0" />
               </children>
            </VBox>
         </content>
      </Tab>
      <Tab fx:id="est" text="Token Separators">
         <content>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Label text="Token Separators:" />
                  <TextArea fx:id="estTokenSeparators" prefHeight="356.0" prefWidth="600.0" />
               </children>
            </VBox>
         </content>
      </Tab>
      <Tab fx:id="sbuild" text="Semantic Dictionary">
         <content>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <VBox prefHeight="367.0" prefWidth="150.0">
                     <children>
                        <Label text="Semantic Node" />
                        <ListView fx:id="sbuildNodes" prefHeight="351.0" prefWidth="150.0" />
                     </children>
                  </VBox>
                  <VBox prefHeight="367.0" prefWidth="450.0">
                     <children>
                        <Label text="Detailes" />
                        <TextArea fx:id="sbuildDetailes" prefHeight="351.0" prefWidth="446.0" />
                     </children>
                  </VBox>
               </children>
            </HBox>
         </content>
      </Tab>
  </tabs>
</TabPane>
